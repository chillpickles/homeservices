variant: fcos
version: 1.5.0
passwd:
  users:
    - name: zmaier
      groups:
        - docker
        - wheel
        - sudo
      ssh_authorized_keys:
        - ssh-rsa 
        - ssh-rsa 
storage:
  files:
    - path: /etc/hostname
      mode: 0644
      contents:
        inline: myhostname
    - path: /etc/zincati/config.d/55-updates-strategy.toml
      contents:
        inline: |
          [updates]
          strategy = "periodic"
          [[updates.periodic.window]]
          days = [ "Mon", "Tue" ]
          start_time = "09:00"
          length_minutes = 60
systemd:
  units:
    - name: rpm-ostree-countme.timer
      enabled: false
      mask: true